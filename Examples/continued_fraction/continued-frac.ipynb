   "source": [
    "a = [3,4,12,4] # example list\n",
    "3+1/(4+1/(12+1/4)) # numerical value of continued fraction"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def cont_frac(lista):\n",
    "    lista.reverse()\n",
    "    cf = lista[0]\n",
    "    for i in lista[1:]:\n",
    "        cf = 1/cf + i\n",
    "    return cf"
   ]
  },
  {
